{
  "name": "papupata",
  "version": "1.8.1",
  "description": "Typescript-typed REST requests and API implementations",
  "homepage": "https://baronagroup.github.io/papupata",
  "repository": {
    "type": "git",
    "url": "BaronaGroup/papupata"
  },
  "license": "MIT",
  "author": {
    "name": "Tapani Haka"
  },
  "peerDependencies": {
    "@types/express": "^4"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.0",
    "@types/express": "4.17.4",
    "@types/jest": "26.0.14",
    "@types/lodash": "4.14.162",
    "@types/qs": "6.9.5",
    "@types/request-promise": "4.1.46",
    "@types/supertest": "2.0.10",
    "@types/uuid": "8.3.0",
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "jest": "26.6.3",
    "npm-release": "1.0.0",
    "request": "2.88.2",
    "request-promise": "4.2.6",
    "supertest": "5.0.0",
    "ts-jest": "26.4.1",
    "typescript": "4.0.2",
    "uuid": "8.3.1"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "qs": "^6.7.0"
  },
  "main": "dist/main/index",
  "types": "dist/main/index.d.ts",
  "scripts": {
    "build": "npx tsc --project tsconfig.json",
    "clean": "rm -rf ./dist",
    "test": "jest",
    "prepare": "if [ -e node_modules ]; then npm run build; fi",
    "prepublishOnly": "npm run clean && npm run build",
    "release": "npm test && npm-release"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>src/**/*.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/*-test.ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node",
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "incremental": true
        }
      }
    }
  }
}
